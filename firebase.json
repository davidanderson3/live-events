{
  "functions": {
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "site": "live-events-6f3e5",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "api"
      },
      {
        "source": "/api/spotify-client-id",
        "function": "api"
      },
      {
        "source": "/contact",
        "function": "api"
      },
      {
        "source": "/daily",
        "function": "api"
      },
      {
        "source": "/countries",
        "function": "api"
      },
      {
        "source": "/layer/**",
        "function": "api"
      },
      {
        "source": "/score",
        "function": "api"
      },
      {
        "source": "/leaderboard",
        "function": "api"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
